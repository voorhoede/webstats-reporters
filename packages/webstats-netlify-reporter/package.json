{
  "name": "webstats-netlify-reporter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config ../../codegen.prod.yml",
    "start": "graphql-codegen --config ../../codegen.dev.yml --watch",
    "build": "ncc build src/index.ts -o dist",
    "save": "node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettier-format && yarn run lint"
    }
  },
  "dependencies": {
    "@webstats-reporters/common": "*",
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-resolvers": "1.18.2",
    "@types/node-fetch": "^2.5.8",
    "@vercel/ncc": "^0.27.0",
    "graphql": "^15.5.0",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "prettier": "^2.2.1"
  }
}
